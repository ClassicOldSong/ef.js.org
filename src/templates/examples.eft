>div.{{class.body}}
	>ul.{{class.list}}#list
		>li.{{class.todo}}
			.TodoMVC
			@click = show:todo
		>li.{{class.dbmon}}
			.dbmon
			@click = show:dbmon
		>li.{{class.site}}
			.ef.js.org
			@click = show:site
		>li.{{class.split}}
		>li.{{class.item}}
			.New window
			@click = open:{{src}}
		>li.{{class.item}}
			.Repository
			@click = open:{{repo}}
	>div.{{class.showbox}}
		>iframe.{{class.show}}
			#src = {{src}}
